		{
			"name": "Crear cliente sin tarjeta",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Guarda el ID del usuario recién creado en una variable global\r",
							"const response = pm.response.json();\r",
							"if (response && response.idUsuario) {\r",
							"    pm.globals.set(\"idUsuario\", response.idUsuario);\r",
							"    console.log(\"✅ ID del usuario guardado: \" + response.idUsuario);\r",
							"} else {\r",
							"    console.log(\"⚠️ No se encontró idUsuario en la respuesta\");\r",
							"}\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
